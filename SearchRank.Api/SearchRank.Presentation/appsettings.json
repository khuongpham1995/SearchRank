{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "Issuer": "ApiIssuer",
    "Audience": "ApiAudience"
  },
  "RateLimiter": {
    "PermitLimit": 10,
    "FixedWindowSecond": 5,
    "QueueLimit": 2
  },
  "ApiAction": {
    "Grouping": "/api",
    "Token": "/token",
    "Searching": "/searching"
  },
  "AllowedHosts": "*"
}
